<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="urn:oasis:names:tc:xliff:document:1.2">
    <attr default="-1.0" format="float" name="aspect_ratio"/>
    <attr default="true" format="boolean" name="handle_touch_event"/>
    <attr default="scale_none" name="scale_mode">
		<enum name="keep_aspect" value="0"/>
		<enum name="stretch_to_fit" value="1"/>
		<enum name="crop_center" value="2"/>
	</attr>

    <style name="SeekBarPreference_summary" parent="@android:style/TextAppearance.Small">
		<!-- <item name="android:textColor">#ffa500</item> -->
	</style>
    <style name="SeekBarPreference_title" parent="@android:style/TextAppearance.Large"/>
</resources>